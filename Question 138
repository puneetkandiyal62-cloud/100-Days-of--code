/138. Print all enum names and integer values using a loop.
#include <stdio.h>
enum UserRole {
    ADMIN,
    USER,
    GUEST,
    TOTAL_ROLES
};

int main() {
    const char *roleNames[] = {"ADMIN", "USER", "GUEST"};
    printf("User Roles:\n");

    for (int i = 0; i < TOTAL_ROLES; i++) {
        printf("%s = %d\n", roleNames[i], i);
    }

    return 0;
}

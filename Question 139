/139. Show that enums store integers by printing assigned values.
#include <stdio.h>
enum UserRole {
    ADMIN = 5,
    USER = 10,
    GUEST = 15
};

int main() {
    printf("Enum values stored internally as integers:\n");
    printf("ADMIN = %d\n", ADMIN);
    printf("USER  = %d\n", USER);
    printf("GUEST = %d\n", GUEST);

    return 0;
}
